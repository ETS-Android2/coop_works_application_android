<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:background="#FFFFFF"
    android:orientation="vertical">

    <TextView
        android:id="@+id/textView33"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_weight="0"
        android:background="#B4B4B4"
        android:gravity="center"
        android:padding="10dp"
        android:text="Prissætning"
        android:textAppearance="@style/TextAppearance.AppCompat.Body2"
        android:textSize="18sp" />

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_weight="0"
        android:orientation="horizontal">

        <TextView
            android:id="@+id/textView42"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:background="#DCDCDC"
            android:gravity="center"
            android:text="rabat (DKK)"
            android:textColor="#2B2B2B"
            android:textSize="18sp" />

        <ImageView
            android:id="@+id/view20"
            android:layout_width="2dp"
            android:layout_height="match_parent"
            android:layout_weight="0"
            android:background="#B4B4B4" />

        <EditText
            android:id="@+id/editText"
            android:layout_width="0dp"
            android:layout_height="50dp"
            android:layout_weight="1"
            android:background="#00FFFFFF"
            android:ems="10"
            android:gravity="center"
            android:hint="0"
            android:inputType="number"
            android:textColor="#2B2B2B" />
    </LinearLayout>

    <ImageView
        android:id="@+id/view15"
        android:layout_width="match_parent"
        android:layout_height="2dp"
        android:layout_weight="0"
        android:background="#B4B4B4" />

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_weight="0"
        android:orientation="horizontal">

        <TextView
            android:id="@+id/textView43"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:background="#DCDCDC"
            android:gravity="center"
            android:text="rabat (%)"
            android:textColor="#2B2B2B"
            android:textSize="18sp" />

        <ImageView
            android:id="@+id/view21"
            android:layout_width="2dp"
            android:layout_height="match_parent"
            android:layout_weight="0"
            android:background="#B4B4B4" />

        <EditText
            android:id="@+id/editText4"
            android:layout_width="0dp"
            android:layout_height="50dp"
            android:layout_weight="1"
            android:background="#00FFFFFF"
            android:ems="10"
            android:gravity="center"
            android:hint="0%"
            android:inputType="number"
            android:textColor="#2B2B2B" />
    </LinearLayout>

    <ImageView
        android:id="@+id/view16"
        android:layout_width="match_parent"
        android:layout_height="2dp"
        android:layout_weight="0"
        android:background="#B4B4B4" />

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_weight="0"
        android:orientation="horizontal">

        <TextView
            android:id="@+id/textView44"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="2"
            android:background="#DCDCDC"
            android:gravity="center"
            android:text="antal (min/max)"
            android:textColor="#2B2B2B"
            android:textSize="18sp" />

        <ImageView
            android:id="@+id/view22"
            android:layout_width="2dp"
            android:layout_height="match_parent"
            android:layout_weight="0"
            android:background="#B4B4B4" />

        <EditText
            android:id="@+id/editText5"
            android:layout_width="0dp"
            android:layout_height="50dp"
            android:layout_weight="1"
            android:background="#00FFFFFF"
            android:ems="10"
            android:gravity="center"
            android:hint="0"
            android:inputType="number"
            android:textColor="#2B2B2B" />

        <EditText
            android:id="@+id/editText6"
            android:layout_width="0dp"
            android:layout_height="50dp"
            android:layout_weight="1"
            android:background="#00FFFFFF"
            android:ems="10"
            android:gravity="center"
            android:hint="0"
            android:inputType="number"
            android:textColor="#2B2B2B" />
    </LinearLayout>

    <ImageView
        android:id="@+id/view17"
        android:layout_width="match_parent"
        android:layout_height="2dp"
        android:layout_weight="0"
        android:background="#B4B4B4" />

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_weight="0"
        android:orientation="horizontal">

        <TextView
            android:id="@+id/textView45"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:background="#DCDCDC"
            android:gravity="center"
            android:text="rabat for"
            android:textColor="#2B2B2B"
            android:textSize="18sp" />

        <ImageView
            android:id="@+id/view23"
            android:layout_width="2dp"
            android:layout_height="match_parent"
            android:layout_weight="0"
            android:background="#B4B4B4" />

        <ToggleButton
            android:id="@+id/toggleButton2"
            android:layout_width="0dp"
            android:layout_height="50dp"
            android:layout_weight="1"
            android:background="#00FFFFFF"
            android:checked="true"
            android:drawableTint="#00FFFFFF"
            android:textColorHighlight="#00FFFFFF"
            android:textOff="alle"
            android:textOn="medlemmer" />
    </LinearLayout>

    <ImageView
        android:id="@+id/view18"
        android:layout_width="match_parent"
        android:layout_height="2dp"
        android:layout_weight="0"
        android:background="#B4B4B4" />

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_weight="0"
        android:orientation="horizontal">

        <TextView
            android:id="@+id/textView46"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:background="#DCDCDC"
            android:gravity="center"
            android:text="gælder (fra/til)"
            android:textColor="#2B2B2B"
            android:textSize="18sp" />

        <ImageView
            android:id="@+id/view24"
            android:layout_width="2dp"
            android:layout_height="match_parent"
            android:layout_weight="0"
            android:background="#B4B4B4" />

        <EditText
            android:id="@+id/editText7"
            android:layout_width="0dp"
            android:layout_height="50dp"
            android:layout_weight="1"
            android:background="#00FFFFFF"
            android:ems="10"
            android:gravity="center"
            android:hint="00/00/00 - 00/00/00"
            android:inputType="number"
            android:textColor="#2B2B2B" />
    </LinearLayout>

    <Button
        android:id="@+id/ScannerPricechangeExecuteBtn"
        android:layout_width="match_parent"
        android:layout_height="60dp"
        android:layout_weight="0"
        android:background="@color/Primary"
        android:insetTop="0dp"
        android:insetBottom="0dp"
        android:text="bekræft prisnedsætning"
        android:textAllCaps="true"
        android:textColor="#FFFFFF" />

</LinearLayout>